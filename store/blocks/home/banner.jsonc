{
  "flex-layout.row#banner": {
    "children": [
      "modal-trigger#banner"
    ],
    "props": {
      "blockClass": "banner",
      "fullWidth": true
    }
  },

  "modal-trigger#banner": {
    "children": [
      "flex-layout.col#banner1",
      "flex-layout.col#banner2",
      "modal-layout#banner"
    ]
  },

  "flex-layout.col#banner1": {
    "children": [
      "info-card#rethink"
    ]
  },
  "flex-layout.col#banner2": {
    "children": [
      "image#deal2"
    ]
  },

  "info-card#rethink": {
    "props": {
      "isFullModeStyle": true,
      "headline": "faça parte do nosso mundo.",
      "callToActionText": "Junte-se a nós!",
      "callToActionMode": "button",
      "textPosition": "left",
      "blockClass": "customCardInfo"
    }
  },

  "image#deal2": {
    "props": {
      "maxHeight": "400px"
    }
  },

  "modal-layout#banner": {
    "children": [
      "modal-header",
      "modal-content"
    ],
    "props":{
      "blockClass": "customModal"
    }
  },

  "modal-header": {
    "children":[
      "rich-text#modal"
    ]
  },

  "modal-content": {
    "children":[
      "formulario-leads"
    ]
  },

  "rich-text#modal": {
    "props": {
      "text": "### Cadastro de Leads"
    }
  }
}